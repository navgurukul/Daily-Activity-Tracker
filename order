\COPY (
    SELECT
        s.name,
        s.email,
        s.partner_id,
        e.total_marks,
        s.created_at,
        s.stage,
        s.district,
        c.mobile
    FROM
        students s
    LEFT JOIN
        contacts c ON s.id = c.student_id
    LEFT JOIN
        enrolment_keys e ON s.id = e.student_id
    LEFT JOIN
