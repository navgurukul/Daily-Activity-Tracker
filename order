\COPY (
    SELECT
        s.email,
        s.partner_id,
        p.name AS partner_name,
        e.total_marks,
        s.stage,
        s.district,
        c.mobile
    FROM
        students s
    LEFT JOIN
        contacts c ON s.id = c.student_id
    LEFT JOIN
        enrolment_keys e ON s.id = e.student_id
    LEFT JOIN
        student_campus sc ON s.id = sc.student_id
    LEFT JOIN
        partners p ON s.partner_id = p.id
    WHERE
        s.partner_id = 899
    GROUP BY
        s.email, s.partner_id, p.name, e.total_marks, s.stage, s.district, c.mobile
) TO '/home/navgurukul/Desktop/jashpurPartner.csv' WITH CSV HEADER;
