    NAME VARCHAR(50),
    NO INT PRIMARY KEY,
    DEPT VARCHAR(50),
    BRANCH VARCHAR(50),
    BASIC DECIMAL(10, 2),
    HRA DECIMAL(10, 2),
    DEDUCTIONS DECIMAL(10, 2),
    TAX DECIMAL(10, 2),
    DATE_OF_JOIN DATE
);
