\COPY (
    SELECT
        s.email,
        s.partner_id,
        e.total_marks,
        s.stage,
        s.district,
        c.mobile
    FROM
        students s
    LEFT JOIN
        contacts c ON s.id = c.student_id
    LEFT JOIN
        enrolment_keys e ON s.id = e.student_id
    LEFT JOIN
        student_campus sc ON s.id = sc.student_id
    WHERE
        e.total_marks::integer >= 10
        AND sc.campus_id = 8
) TO '/home/navgurukul/Desktop/abhove10file.csv' WITH CSV HEADER;
\COPY (
    SELECT
        s.email,
        s.partner_id,
        p.name AS partner_name,
        e.total_marks,
        s.stage,
        s.district,
        c.mobile
    FROM
        students s
    LEFT JOIN
        contacts c ON s.id = c.student_id
    LEFT JOIN
        enrolment_keys e ON s.id = e.student_id
    LEFT JOIN
        student_campus sc ON s.id = sc.student_id
    LEFT JOIN
        partners p ON s.partner_id = p.id
    WHERE
        e.total_marks::integer >= 10
        AND sc.campus_id = 8
        AND s.partner_id = 899
) TO '/home/navgurukul/Desktop/abhove10file.csv' WITH CSV HEADER;
